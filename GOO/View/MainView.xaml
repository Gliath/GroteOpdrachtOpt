<Window x:Class="GOO.View.MainView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Progress" MinHeight="81" Height="128" Width="256">
    <Grid>
        <ProgressBar Margin="10,10,10,10" Maximum="{Binding Path=ProgressMaximum}" Minimum="0" Value="{Binding Path=ProgressValue}" />
    </Grid>
</Window>